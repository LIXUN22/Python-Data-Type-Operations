   "source": [
    "# write a program to swap 2 variables with and without using a 3rd variable\n",
    "a = 2\n",
    "b = 5\n",
    "\n",
    "#  without 3rd variable\n",
    "a,b = b,a\n",
    "print(a)\n",
    "print(b)\n",
    "\n",
    "#  using 3rd variable\n",
    "c = a\n",
    "a = b\n",
    "b = c\n",
    "print(a)\n",
    "print(b)"
   ]
